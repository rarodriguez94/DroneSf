{% extends 'base.html.twig' %}
{% block stylesheets %}
    {% stylesheets filter="sass" filter='?uglifycss' filter='cssrewrite' output='css/drone.min.css'
        "styles/sass/globals.scss"
        "styles/sass/style.scss"
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}
{% block body %}
    Homepage.
{% endblock %}

{% block javascripts %}
    {% javascripts filter='?uglifyjs2' output='js/drone.min.js'
        "scripts/foundation.js"
        "scripts/jquery-2.1.1.min.js" %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

